        <Canvas x:Name="CanvasReloj"  HorizontalAlignment="Center" VerticalAlignment="Center" >
            <Line x:Name="AgujaHoras" Stroke="White" StrokeThickness="6"  X1="0" Y1="0" X2="0" Y2="-128" Canvas.Left="-2"  >
                <Line.Effect>
                    <DropShadowEffect Color="#444444" Opacity="2" Direction="{Binding ElementName=AgujaHorasRotateTransform, Path=Angle}" RenderingBias="Quality" ShadowDepth="1" />
                </Line.Effect>
                <Line.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform  />
                        <SkewTransform/>
                        <RotateTransform x:Name="AgujaHorasRotateTransform" CenterX="0" CenterY="0" Angle="0" />
                    </TransformGroup>
                </Line.RenderTransform>
                <Line.Triggers>
                    <EventTrigger RoutedEvent="Line.Loaded">
                        <BeginStoryboard>
                            <Storyboard TargetProperty="Angle">
                                <DoubleAnimation AutoReverse="False" By="360" IsAdditive="True" Duration="12:0:0" RepeatBehavior="Forever"  Storyboard.TargetName="AgujaHorasRotateTransform" Storyboard.TargetProperty="Angle" />
                            </Storyboard>
                        </BeginStoryboard>
                    </EventTrigger>
                </Line.Triggers>
